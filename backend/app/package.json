    "express-session": "^1.17.3",
    "firebase-admin": "^12.1.0",
    "jsonwebtoken": "^9.0.2",
    "memorystore": "^1.6.7",
    "mysql2": "^2.0.2",
    "rdis": "^0.0.1-security",
    "redis": "^4.6.13",
    "sequelize": "^5.22.5",
    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/node": "^20.10.4",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "swagger-jsdoc": "^6.2.8",
